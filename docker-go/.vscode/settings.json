{
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": true,
  "files.insertFinalNewline": true,
  "editor.renderLineHighlight": "all",
  "cSpell.ignoreWords": [
    "hoge",
    "myzn",
    "psql"
  ],
  "cSpell.words": [],
  "editor.tabSize": 4,
  "files.autoSave": "off",
  "editor.tabFocusMode": false,
  "go.formatTool": "gofmt",
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnSave": true
  }
}
